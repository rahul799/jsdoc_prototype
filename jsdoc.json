{
    "source": {
      "include": ["src"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
    },
    "plugins": ["./node_modules/tsd-jsdoc/dist/plugin"],
    "opts": {
      "recurse": true,
      "destination": "./docs/",
      "template": "./node_modules/tsd-jsdoc/dist"
    }
  }
  